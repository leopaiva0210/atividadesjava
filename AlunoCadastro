package trabalhocadastro;


public class AlunoCadastro {
    
    String nome;
    int cod;
    String email;
    String cidade;
    String uf;
    Alunos(String nome, int cod,String email, String cidade,String uf){
    this.nome=nome;
    this.codigo=cod;
    this.email=email;
    this.cidade=cidade;
    this.uf=uf;
}
}
